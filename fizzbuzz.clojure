#!/usr/bin/clojure

; Language: Clojure
; Web site: http://clojure.org/
; Ubuntu:   apt-get install clojure
; Linux Mint 17: apt-get install clojure1.4

(defn line [n]
    (cond
        (= 0 (mod n 15)) "FizzBuzz"
        (= 0 (mod n  3)) "Fizz"
        (= 0 (mod n  5)) "Buzz"
        :else (format "%d" n)
    )
)

(defn print-line [n] (println (line n)))

(dorun (map print-line (range 1 101)))
